      "x-nullable": true
    },
    "ThreadId": {
      "type": "string",
      "x-nullable": true
    },
    "LlmConnector": {
      "type": "string",
      "x-nullable": true
    },
    "Prompt": {
      "type": "string",
      "x-nullable": true
    },
    "Url": {
      "type": "string",
      "x-nullable": true
    },
    "Language": {
      "type": "string",
      "x-nullable": true
    },
    "Description": {
      "type": "string",
      "x-nullable": true
    },
    "Variables": {
